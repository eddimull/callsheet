{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:vue/vue3-recommended",
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        "max-len": 0,
        "linebreak-style": 0,
        "no-shadow": "off",
        "js-doc": 0,
        "valid-jsdoc": [2, {
            "prefer": {
                "return": "return"
            }
        }],
        "vue/multi-word-component-names": "off"
    }
}
